{
  "name": "camsearch",
  "version": "1.0.0",
  "private": true,
  "main": "electron-main.js",
  "author": {
    "name": "Gitansh Bhanot",
    "email": "test@gmail.com"
  },
  "homepage": "./",
  "description": "Camera search utility",
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.16.4",
    "@mui/x-data-grid": "^6.16.1",
    "@mui/x-data-grid-pro": "^7.12.1",
    "@mui/x-date-pickers": "^7.19.0",
    "@mui/x-date-pickers-pro": "^7.19.0",
    "@mui/x-license": "^7.23.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@uidotdev/usehooks": "^2.1.1",
    "apexcharts": "^4.4.0",
    "axios": "^1.1.3",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.3.1",
    "electron-log": "^5.2.4",
    "electron-notification": "^1.0.0",
    "electron-progressbar": "^2.2.1",
    "electron-store": "^8.1.0",
    "electron-unhandled": "^4.0.1",
    "electron-updater": "^6.3.9",
    "file-saver": "^2.0.5",
    "framer-motion": "^7.5.3",
    "lottie-react": "^2.4.1",
    "mixpanel-browser": "^2.48.1",
    "node-onvif": "^0.1.7",
    "react": "^18.2.0",
    "react-apexcharts": "^1.6.0",
    "react-card-flip": "^1.2.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.1",
    "react-zoom-pan-pinch": "^3.6.1",
    "wait-on": "^8.0.2",
    "web-vitals": "^2.1.4"
  },
  "build": {
    "appId": "com.demo.camsearch",
    "productName": "Camera Search",
    "extends": null,
    "directories": {
      "buildResources": "public",
      "output": "dist"
    },
    "extraResources": [
      {
        "from": "./scripts/",
        "to": "scripts/",
        "filter": [
          "**/*"
        ]
      }
    ],
    "asarUnpack": [
      "electron-main.js",
      "preload.js"
    ],
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "electron/**/*",
      "electron-main.js",
      "preload.js"
    ],
    "mac": {
      "category": "public.app-category.business",
      "icon": "public/icon.icns",
      "target": "dmg",
      "hardenedRuntime": true,
      "gatekeeperAssess": false
    },
    "win": {
      "target": "nsis",
      "icon": "public/app-icon.ico"
    },
    "nsis": {
      "runAfterFinish": false
    }
  },
  "scripts": {
    "start": "vite --host",
    "build": "vite build",
    "serve": "vite preview",
    "electron:start": "concurrently -k \"cross-env BROWSER=none npm start\" \"wait-on http://127.0.0.1:3000 && electron . --force_high_performance_gpu\"",
    "electron-build": "electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@tailwindcss/typography": "^0.5.10",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "autoprefixer": "^10.4.20",
    "electron": "^28.2.1",
    "electron-builder": "^24.6.4",
    "electron-notarize": "^1.2.2",
    "eslint": "^9.11.1",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.3.2",
    "vite": "^5.4.10"
  }
}
